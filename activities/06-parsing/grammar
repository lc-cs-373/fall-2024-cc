skip WS '\s+'
token NUM '\d+'
token LEFTBRACKET '\['
token RIGHTBRACKET '\]'
token COMMA ','

%

<lon> ::= LEFTBRACKET <nums> RIGHTBRACKET
<nums> **= <NUM>

#<nums>:NumsNode ::= <NUM> <nums>
#<nums>:NumsNull ::= 

%

Lon
%%%
public void $run(){
    System.out.println(this.nums.toString());
}
